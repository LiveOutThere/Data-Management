        <Field Name="tax_amount">
          <DataField>tax_amount</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT LEFT(created_at,11) AS created_at, increment_id, grand_total, subtotal, tax_amount
FROM tbl_Magento_Sales AS a
INNER JOIN lut_Date AS b ON LEFT(a.created_at,11) = b.DateFull
WHERE b.PeriodKey = @DatePeriod
ORDER BY created_at DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@DatePeriod">
            <Value>=Parameters!DatePeriod.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
