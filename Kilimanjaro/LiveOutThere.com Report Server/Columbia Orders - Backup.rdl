        </Field>
        <Field Name="items">
          <DataField>items</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="order_value">
          <DataField>order_value</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="order_value_incl_tax">
          <DataField>order_value_incl_tax</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     created_at, increment_id, SUM(qty_ordered) AS items, ROUND(SUM(price), 2) AS order_value, ROUND(SUM(price_incl_tax), 2) AS order_value_incl_tax
FROM         tbl_Magento_Sales_Items
WHERE     (sku LIKE 'COL%')
GROUP BY created_at, increment_id
ORDER BY created_at DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
