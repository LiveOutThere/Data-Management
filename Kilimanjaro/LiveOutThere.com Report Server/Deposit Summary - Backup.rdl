      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT  DISTINCT last_captured_date,  cc_type, captured_today_this_cc_type, 
                      refunded_today_this_cc_type, expected_deposit_today_this_cc_type
FROM         view_Bank_Reconciliation WHERE PeriodKey = @PeriodKey ORDER BY last_captured_date, cc_type</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@PeriodKey">
            <Value>=Parameters!PeriodKey.Value</Value>
