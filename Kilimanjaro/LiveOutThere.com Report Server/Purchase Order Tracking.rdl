    <DataSet Name="Season">
      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <CommandText>SELECT NULL AS season
UNION ALL
SELECT po_carrier AS season
FROM OPENQUERY (MAGENTO,'SELECT DISTINCT po_carrier FROM purchase_order WHERE po_status NOT IN(''closed'',''complete'',''cancelled'')')
				</CommandText>
      </Query>
      <Fields>
        <Field Name="season">
          <DataField>season</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Type">
      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <CommandText>SELECT po_type AS type
FROM OPENQUERY (MAGENTO,'SELECT DISTINCT po_type FROM purchase_order')
ORDER BY type	</CommandText>
      </Query>
      <Fields>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Status">
      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <CommandText>SELECT NULL AS status
UNION ALL
SELECT CASE WHEN po_status = 'new' THEN 'New'
	      WHEN po_status = 'waiting_to_receive' THEN 'Arrived'
	      WHEN po_status = 'in_progress' THEN 'In Progress'
 	      WHEN po_status = 'acknowledged' THEN 'Acknowledged'
	      ELSE po_status END AS status
FROM OPENQUERY (MAGENTO,'SELECT DISTINCT po_status FROM purchase_order WHERE po_status NOT IN(''complete'',''closed'',''cancelled'')')
ORDER BY status		</CommandText>
      </Query>
      <Fields>
        <Field Name="status">
          <DataField>status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
