          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="qty">
          <DataField>qty</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <CommandText>SELECT DISTINCT LEFT(available_from,10) AS value, LEFT(available_from,10) AS label
FROM         report_Ops_Deals
ORDER BY value DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="label">
          <DataField>label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
