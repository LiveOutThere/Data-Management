  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Date">
            <Value>=Parameters!Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT * FROM report_Ops_Deals
WHERE available_from &gt;= @Date AND available_from &lt;= DATEADD(d,3,@Date)
ORDER BY CAST(available_from AS date), product_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="available_from">
          <DataField>available_from</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="available_to">
