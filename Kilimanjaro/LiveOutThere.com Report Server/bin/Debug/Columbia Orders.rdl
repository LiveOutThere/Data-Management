  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>LOT_Reporting</DataSourceName>
        <CommandText>SELECT     created_at, increment_id, SUM(qty_ordered) AS items, ROUND(SUM(price), 2) AS order_value, ROUND(SUM(price_incl_tax), 2) AS order_value_incl_tax
FROM         tbl_Magento_Sales_Items
WHERE     (sku LIKE 'COL%')
GROUP BY created_at, increment_id
ORDER BY created_at DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="created_at">
          <DataField>created_at</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="increment_id">
